<Project>
	<PropertyGroup Condition="'$(Version)' == ''">
		<Version>1.0.0.0</Version>
	</PropertyGroup>
	<PropertyGroup>
		<ArtifactsDir Condition="'$(ArtifactsDir)' == ''">$(SolutionDir)..\bin\$(SolutionName)</ArtifactsDir>
		<OutputPath>$(ArtifactsDir)\$(TargetFramework)</OutputPath>
		<AppendTargetFrameworkToOutputPath>False</AppendTargetFrameworkToOutputPath>
		<BaseIntermediateOutputPath>$(SolutionDir)\..\obj\$(MSBuildProjectName)\$(TargetFramework)</BaseIntermediateOutputPath>
	</PropertyGroup>
</Project>